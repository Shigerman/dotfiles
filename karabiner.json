            "modifiers": {"mandatory": ["left_control"]}
          },
          "to": [{
            "key_code": "f",
            "modifiers": ["command"]
          }]
        },

        {
          "description": "remap ctrl-s to command-s",
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {"mandatory": ["left_control"]}
          },
          "to": [{
            "key_code": "s",
            "modifiers": ["command"]
          }]
        },

        {
          "description": "remap ctrl-a to command-a",
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {"mandatory": ["left_control"]}
          },
          "to": [{
            "key_code": "a",
