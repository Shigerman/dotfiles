        },

        {
          "description": "remap ctrl-v to command-v",
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {"mandatory": ["left_control"]}
          },
          "to": [{
            "key_code": "v",
            "modifiers": ["command"]
          }]
        },

        {
          "description": "remap ctrl-f to command-f",
          "type": "basic",
          "from": {
