          "type": "basic",
          "from": {
            "key_code": "z",
            "modifiers": {"mandatory": ["left_control"]}
          },
          "to": [{
            "key_code": "z",
            "modifiers": ["command"]
          }]
        }

        ]
      }]
    },
    "devices": [],
    "fn_function_keys": [],
    "name": "Default profile",
    "parameters": {
      "delay_milliseconds_before_open_device": 1000
    },
    "selected": true,
    "simple_modifications": [],
    "virtual_hid_keyboard": {
      "country_code": 0,
      "mouse_key_xy_scale": 100
    }
  }]
