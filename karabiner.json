{
  "global": {
    "check_for_updates_on_startup": true,
    "show_in_menu_bar": true,
    "show_profile_name_in_menu_bar": false
  },
  "profiles": [{
    "complex_modifications": {
      "parameters": {
        "basic.simultaneous_threshold_milliseconds": 50,
        "basic.to_delayed_action_delay_milliseconds": 500,
        "basic.to_if_alone_timeout_milliseconds": 1000,
        "basic.to_if_held_down_threshold_milliseconds": 500,
        "mouse_motion_to_scroll.speed": 100
      },
      "rules": [{
        "description": "General keyboard modifications",
        "manipulators": [

        {
          "description": "Use caps loxk as 'meta' key",
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
            "set_variable": {
              "name": "_m",
              "value": 1
            }
          }],
          "to_after_key_up": [{
            "set_variable": {
              "name": "_m",
              "value": 0
            }
          }]
        },

        {
          "description": "meta-open-bracket for escape (vim-like)",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
            "key_code": "escape"
          }],
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "type": "basic"
        },

        {
          "description": "meta-delete for closing apps",
          "from": {
            "key_code": "delete_or_backspace"          },
          "to": [{
            "key_code": "q",
            "modifiers": ["left_command"]
          }],
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "type": "basic"
        },

        {
          "description": "meta-p for backspace",
          "from": {
            "key_code": "p"
          },
          "to": [{
            "key_code": "delete_or_backspace"
          }],
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "type": "basic"
        },

        {
          "description": "meta-semicolon for left mouse button",
          "from": {
            "key_code": "semicolon",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
            "pointing_button": "button1"
          }],
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "type": "basic"
        },

        {
          "description": "meta-quote for right mouse button",
          "from": {
            "key_code": "quote",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
            "pointing_button": "button2"
          }],
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "type": "basic"
        },

        {
          "description": "neta-shift-h for shift-left-arrow",
          "from": {
            "key_code": "h",
            "modifiers": {"mandatory": ["shift"]}
          },
          "to": [{
            "key_code": "left_arrow",
            "modifiers": ["shift"]
          }],
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "type": "basic"
        },

        {
          "description": "neta-h for left-arrow",
          "from": {
            "key_code": "h",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
            "key_code": "left_arrow"
          }],
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "type": "basic"
        },

        {
          "description": "neta-shift-j for shift-down-arrow",
          "from": {
            "key_code": "j",
            "modifiers": {"mandatory": ["shift"]}
          },
          "to": [{
            "key_code": "down_arrow",
            "modifiers": ["shift"]
          }],
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "type": "basic"
        },

        {
          "description": "neta-j for down-arrow",
          "from": {
            "key_code": "j",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
            "key_code": "down_arrow"
          }],
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "type": "basic"
        },

        {
