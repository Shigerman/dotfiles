            "key_code": "k",
            "modifiers": {"mandatory": ["shift"]}
          },
          "to": [{
            "key_code": "up_arrow",
            "modifiers": ["shift"]
          }],
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "type": "basic"
        },

        {
          "description": "neta-k for up-arrow",
          "from": {
            "key_code": "k",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
            "key_code": "up_arrow"
          }],
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
